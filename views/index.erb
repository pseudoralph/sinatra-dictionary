<div class="row interface_nav">
  <div class="col mx-auto">

    <form action="/dictionary" method="get" target="view_frame">
      <button type="submit" id="view" class="styled_buttons" >
        <i class="fas fa-glasses"></i>
      </button>
      <p>View all words</p>
    </form>

  </div>

  <div class="col mx-auto">

    <form action="/dictionary/add_word" method="get" target="view_frame">
      <button type="submit" class="styled_buttons" id="add"><i class="fas fa-plus"></i></button>
      <p>Add new word</p>

    </form>
  </div>

</div>

<div class="container-fluid app_box">
  <iframe src="/dictionary" width="100%" height="100%" name="view_frame" frameborder="0"></iframe>
</div>
<div class="">
  <small class="low_vis"><a href="/console" target="view_frame">options</a></small>

</div>
