<div class="container">

  <div class="row">
    <div class="col-md-6">

      <form action="/dictionary" method="get" target="view_frame">
        <button type="submit" class="btn btn-primary">View Dictionary</button>
      </form>

    </div>

    <div class="col-md-6">

      <form action="/dictionary/add_word" method="get" target="view_frame">
        <button type="submit" class="btn btn-primary">Add Word</button>
      </form>
    </div>

  </div>

  <div class="row">
    <iframe src="/dictionary" width="90%" height="600px" name="view_frame"></iframe>

  </div>


</div>
