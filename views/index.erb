<div class="app">


<div class="row button_nav">
  <div class="col mx-auto">

    <form action="/dictionary" method="get" target="view_frame">
      <button type="submit">
        <i class="fas fa-glasses"></i>
      </button>
      <p>View all words</p>
    </form>

  </div>

  <div class="col mx-auto">

    <form action="/dictionary/add_word" method="get" target="view_frame">
      <button type="submit"><i class="fas fa-plus"></i></button>
      <p>Add new word</p>

    </form>
  </div>

</div>

<div class="container-fluid app_box">
  <iframe src="/dictionary" height="100%" width="100%" name="view_frame" frameborder="1"></iframe>
</div>

<div class="options">
  <small><a href="/console" target="view_frame">options</a></small>

</div>
</div>
